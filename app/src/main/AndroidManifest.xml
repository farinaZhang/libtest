<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.farina.libtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.farina.activity.UserLoadOnActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name="com.farina.activity.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.farina.activity.UserInfoActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.farina.activity.BindingUserInfoActivity" />
        <activity android:name="com.farina.activity.ChangeUserPicActivity" />
        <activity android:name="com.farina.activity.ShowInOtherStyleActivity" />
        <activity android:name="com.farina.activity.InstantChatActivity"></activity>
    </application>

</manifest>